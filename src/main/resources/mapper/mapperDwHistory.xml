<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dwHistory">

    <select id="selectAll" resultType="DownloadHistoryVo">
        select * from rsss."download_history" where validity is true order by dw_id
    </select>

    <select id="select" resultType="DownloadHistoryVo">
        select * from rsss."download_history"
        where
                and dw_id = #{dw_id}
    </select>

    <insert id="insert" parameterType="DownloadHistoryVo">
        insert into rsss."download_history" (
                dw_user
                ,dw_date
                ,dw_type
                ,dw_filelist
                ,validity
            ) values (
                #{username}
                ,now()
                ,#{dwType}
                ,#{dwFileList}
                ,true
        )
    </insert>

    <delete id="delete">
        delete
            from rsss."download_history"
            where dw_id = #{dw_id}
    </delete>

</mapper>